CC       = gcc
CFLAGS   = -Wall -g
TARGET   = Isaac_testFS

# If your Terminal library requires linking, add e.g. -lTerminal
LDLIBS   = -lTerminal

all: $(TARGET)

$(TARGET): Isaac_libFS.o Isaac_testFS.o
	$(CC) $(CFLAGS) -o $@ $^ $(LDLIBS)

Isaac_libFS.o: Isaac_libFS.c Isaac_libFS.h
	$(CC) $(CFLAGS) -c $<

Isaac_testFS.o: Isaac_testFS.c Isaac_libFS.h
	$(CC) $(CFLAGS) -c $<

clean:
	rm -f *.o $(TARGET)